
<%- include("./partials/loginhead"); %>

    
	
	<div class="limiter">
		<div class="container-login100">
			<div class="wrap-login100 p-l-55 p-r-55 p-t-65 p-b-50">
				<form action="#" class="login100-form validate-form">
					<span class="login100-form-title p-b-33">
						<a  href="/" class="btn btn-danger">Back</a>

						Withdrawal
					</span>

					<div class="wrap-input100 validate-input" data-validate = "Valid Account Name is required">
						<input class="input100" type="text" name="acctname" placeholder="Account Name">
						<span class="focus-input100-1"></span>
						<span class="focus-input100-2"></span>
					</div>

					<div class="wrap-input100 validate-input" data-validate = "Valid Account Number is required">
						<input class="input100" type="text" name="acctnum" placeholder="Account Number">
						<span class="focus-input100-1"></span>
						<span class="focus-input100-2"></span>
					</div>

					<div class="wrap-input100 rs1">
						<input class="input100" type="text" name="amt" value="20000">
						<span class="focus-input100-1"></span>
						<span class="focus-input100-2"></span>
					</div>

					<div class="wrap-input100 rs1 validate-input" data-validate="Bank Name is required">
						<input class="input100" type="text" name="bankname" placeholder="Bank Name">
						<span class="focus-input100-1"></span>
						<span class="focus-input100-2"></span>
					</div>

					<div class="wrap-input100 rs1 validate-input" data-validate="Password is required">
						<input class="input100" type="password" name="confirmpass" placeholder="Confirm Password">
						<span class="focus-input100-1"></span>
						<span class="focus-input100-2"></span>
					</div>


					

					<div class="wrap-input100 rs1 validate-input" data-validate="Phone Number is required">
						<input class="input100" type="tel" name="telephone" placeholder="Phone Number">
						<span class="focus-input100-1"></span>
						<span class="focus-input100-2"></span>
					</div>


					

					 


					<div>
						<p>Withdrawal might take up to 48 hour</p>
					</div>
					

					<div class="container-login100-form-btn m-t-20">
						<button class="login100-form-btn">
						Withdraw
						</button>
					</div>

					

					
				</form>
			</div>
		</div>
	</div>
	

	<%- include("./partials/logintail"); %>



