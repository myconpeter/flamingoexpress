
<%- include("./partials/loginhead"); %>

    <form action="/withdraw" method="POST">
	
	<div class="limiter">
		<div class="container-login100">
			<div class="wrap-login100 p-l-55 p-r-55 p-t-65 p-b-50">
				<form class="login100-form validate-form">
					<span class="login100-form-title p-b-33">
						<a  href="/myinvestment" class="btn btn-danger">Back</a>

						Withdrawal
					</span>
					<%- include ('./partials/messages') %>

					<div class="wrap-input100 validate-input" data-validate = "Valid Email Name is required">
						<input class="input100" type="email" 
						name="email"
						placeholder="Email Address"
						value="<%= typeof email != 'undefined' ? email : '' %>">
						<span class="focus-input100-1"></span>
						<span class="focus-input100-2"></span>
					</div>

					<div class="wrap-input100 validate-input" data-validate = "Valid Account Name is required">
						<input class="input100" type="acctname" 
						name="acctname"
						placeholder="Account Name"
						value="<%= typeof acctname != 'undefined' ? acctname : '' %>">
						<span class="focus-input100-1"></span>
						<span class="focus-input100-2"></span>
					</div>

					<div class="wrap-input100 validate-input" data-validate = "Valid Account Number is required">
						<input class="input100" type="acctnum" 
						name="acctnum" 
						placeholder="Account Number"
						value="<%= typeof acctnum != 'undefined' ? acctnum : '' %>">
						
						<span class="focus-input100-1"></span>
						<span class="focus-input100-2"></span>
					</div>

					<div class="wrap-input100 rs1">
						<input class="input100" type="hidden" name="amount">
						<span class="focus-input100-1"></span>
						<span class="focus-input100-2"></span>
					</div>

					<div class="wrap-input100 rs1 validate-input" data-validate="Bank Name is required">
						<input class="input100" type="bankname" 
						name="bankname" 
						placeholder="Bank Name"
						value="<%= typeof bankname != 'undefined' ? bankname : '' %>">
						
						<span class="focus-input100-1"></span>
						<span class="focus-input100-2"></span>
					</div>

					<div class="wrap-input100 rs1 validate-input" data-validate="Phone Number is required">
						<input class="input100" type="telephone" 
						name="telephone" 
						placeholder="Phone Number"
						value="<%= typeof telephone != 'undefined' ? telephone : '' %>">
						
						<span class="focus-input100-1"></span>
						<span class="focus-input100-2"></span>
					</div>

					<div class="wrap-input100 rs1 validate-input" data-validate="Secret is required">
						<input class="input100" type="secret" 
						name="secret" 
						placeholder="Secret"
						value="<%= typeof secret != 'undefined' ? secret : '' %>">
						
						<span class="focus-input100-1"></span>
						<span class="focus-input100-2"></span>
					</div>

					<div>
						<input type="hidden" value="<%= currentUser.totalAmount %>">
					</div>


					

					


					

					 


					<div>
						<p>Withdrawal might take up to 48 hour</p>
					</div>
					

					<div class="container-login100-form-btn m-t-20">
						<button class="login100-form-btn" type="submit"> 
						Withdraw
						</button>
					</div>

					

					
				</form>
			</div>
		</div>
	</div>

</form>
	

	<%- include("./partials/logintail"); %>



